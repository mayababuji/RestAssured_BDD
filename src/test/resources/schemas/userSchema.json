{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
      "userAddress": {
        "type": "object",
        "properties": {
          "addressId": { "type": "integer" },
          "plotNumber": { "type": "string" },
          "street": { "type": "string" },
          "state": { "type": "string" },
          "country": { "type": "string" },
          "zipCode": { "type": "integer" }
        },
        "required": ["addressId", "plotNumber", "street", "state", "country", "zipCode"]
      },
      "userId": { "type": "integer" },
      "userFirstName": { "type": "string" },
      "userLastName": { "type": "string" },
      "userContactNumber": { "type": "integer" },
      "userEmailId": { "type": "string", "format": "email" },
      "creationTime": { "type": "string" },
      "lastModTime": { "type": "string" }
    },
    "required": ["userId", "userFirstName", "userLastName", "userContactNumber", "userEmailId", "creationTime", "lastModTime", "userAddress"]
  }
  